  <body>
    <h1 class="center">CROD</h1>
    <div class="container">
      <form  action="/update" method="post">
        <div class="form-group">
          <label for="email" name>isbn:</label>
          <input type="text" class="form-control" id="email" name="isbn" value="<%= user.isbn %>" >
        </div>
        <div class="form-group">
          <label for="pwd">Title:</label>
          <input type="text" class="form-control" id="pwd" name ="title" value="<%= user.title %>">
        </div>
        <div class="form-group">
          <label for="email">author:</label>
          <input type="text" class="form-control" id="email" name ="author" value="<%= user.author %>">
        </div>
        <div class="form-group">
          <label for="pwd">category:</label>
          <input type="text" class="form-control" id="pwd" name="category" value="<%= user.category %>">
        </div>
        <button type="submit" class="btn btn-default">Submit</button>
      </form>
    </div>
  </body>
